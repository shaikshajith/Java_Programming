import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner obj = new Scanner(System.in);
        int a =  obj.nextInt();
        int y=a/365;
        a=a%365;
        int m=a/30;
        a=a%30;
        int w=a/7;
        a=a%7;
        int d=a;
        System.out.println(y+" "+"year(s)"+" "+m+" "+"month(s)"+" "+w+" "+"week(s)"+" "+d+" "+"day(s)");
        
    }
}
